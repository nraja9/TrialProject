[project]
	description = This is a trial project repository
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/nraja (nraja9)
[access "refs/for/refs/*"]
	read = group user/nraja (nraja9)
	push = group user/nraja (nraja9)
	label-Code-Review = -2..+2 group user/nraja (nraja9)
	label-Verified = -1..+1 group user/nraja (nraja9)
	rebase = group user/nraja (nraja9)
	removeReviewer = group user/nraja (nraja9)
	submit = group user/nraja (nraja9)
[access "refs/heads/*"]
	read = group user/nraja (nraja9)
	create = group user/nraja (nraja9)
	push = +force group user/nraja (nraja9)
	pushMerge = group user/nraja (nraja9)
[access "refs/tags/*"]
	push = group user/nraja (nraja9)
	push = +force group user/nraja (nraja9)
